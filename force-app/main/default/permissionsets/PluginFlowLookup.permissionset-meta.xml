<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <label>Plugin Flow Lookup</label>
    <description>Assign this permission to all users that should use the flow lookup plugin
        functionality</description>
    <hasActivationRequired>false</hasActivationRequired>
    <pageAccesses>
        <apexPage>UserSessionPage</apexPage>
        <enabled>true</enabled>
    </pageAccesses>
    <classAccesses>
        <apexClass>PluginFlowLookupCallable</apexClass>
        <enabled>true</enabled>
    </classAccesses>
</PermissionSet>